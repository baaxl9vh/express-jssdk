{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        },
        "object-literal-sort-keys": false,
        "no-console": false,
        "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}